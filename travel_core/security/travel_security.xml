<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">
    <record id="module_category_travel_travel" model="ir.module.category">
        <field name="description">Helps you manage your travel operations.</field>
        <field name="sequence">4</field>
        <field name="name">Travel</field>
    </record>

    <record id="group_travel_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_travel_travel"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_travel_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_category_travel_travel"/>
        <field name="implied_ids" eval="[(4, ref('travel_core.group_travel_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

</odoo>
