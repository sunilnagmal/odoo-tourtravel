<odoo>
    <record id="product_activity_hiking" model="product.template">
        <field name="name">Hiking Tour</field>
        <field name="type">service</field>
        <field name="categ_id" ref="product_category_activity" />
        <field name="list_price">50.00</field>
        <field name="default_code">ACT-HIKE</field>
        <field name="sale_ok">True</field>
        <field name="purchase_ok">True</field>
        <field name="description_sale">Guided hiking tour in the mountains.</field>
        <field name="travel_type">activity</field>

    </record>

    <record id="product_activity_citytour" model="product.template">
        <field name="name">City Tour</field>
        <field name="type">service</field>
        <field name="categ_id" ref="product_category_activity" />
        <field name="list_price">30.00</field>
        <field name="default_code">ACT-CITY</field>
        <field name="sale_ok">True</field>
        <field name="purchase_ok">True</field>
        <field name="description_sale">Walking tour through the city center.</field>
        <field name="travel_type">activity</field>

    </record>

    <record id="product_activity_cooking" model="product.template">
        <field name="name">Cooking Class</field>
        <field name="type">service</field>
        <field name="categ_id" ref="product_category_activity" />
        <field name="list_price">80.00</field>
        <field name="default_code">ACT-COOK</field>
        <field name="sale_ok">True</field>
        <field name="purchase_ok">True</field>
        <field name="description_sale">Local cuisine cooking experience.</field>
        <field name="travel_type">activity</field>

    </record>
</odoo>