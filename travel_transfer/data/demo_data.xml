<odoo>
    <record id="product_transfer_airport_city" model="product.template">
        <field name="name">Airport to City Transfer</field>
        <field name="type">service</field>
        <field name="categ_id" ref="product_category_transfer" />
        <field name="list_price">50.00</field>
        <field name="default_code">TRANSFER-AP-CITY</field>
        <field name="sale_ok">True</field>
        <field name="purchase_ok">False</field>
        <field name="description_sale">Private transfer from airport to city center.</field>
        <field name="travel_type">transfer</field>
    </record>

    <record id="product_transfer_city_hotel" model="product.template">
        <field name="name">City to Hotel Transfer</field>
        <field name="type">service</field>
        <field name="categ_id" ref="product_category_transfer" />
        <field name="list_price">30.00</field>
        <field name="default_code">TRANSFER-CITY-HOTEL</field>
        <field name="sale_ok">True</field>
        <field name="purchase_ok">False</field>
        <field name="description_sale">Comfortable transfer from city center to hotel.</field>
        <field name="travel_type">transfer</field>
    </record>

    <record id="product_transfer_hotel_airport" model="product.template">
        <field name="name">Hotel to Airport Transfer</field>
        <field name="type">service</field>
        <field name="categ_id" ref="product_category_transfer" />
        <field name="list_price">45.00</field>
        <field name="default_code">TRANSFER-HOTEL-AP</field>
        <field name="sale_ok">True</field>
        <field name="purchase_ok">False</field>
        <field name="description_sale">Reliable transfer from hotel to airport.</field>
        <field name="travel_type">transfer</field>
    </record>
</odoo>
