<?xml version="1.0"?>
<odoo>
	<!-- Plans -->
	<record id="product_attribute_meal_plan" model="product.attribute">
		<field name="name">Meal plan</field>
	</record>
	<record id="attribute_value_meal_plan_room_only" model="product.attribute.value">
		<field name="name">Room only</field>
		<field name="attribute_id" ref="product_attribute_meal_plan" />
	</record>

	<record id="attribute_value_meal_plan_breakfast" model="product.attribute.value">
