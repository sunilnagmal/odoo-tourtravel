		<field name="attribute_id" ref="product_attribute_room_type" />
	</record>

	<record id="product_attribute_value_2_br_suite" model="product.attribute.value">
		<field name="name">2 BR suite</field>
		<field name="attribute_id" ref="product_attribute_room_type" />
	</record>

	<record id="product_attribute_value_jr_suite" model="product.attribute.value">
